#include<stdio.h>
int main(int argc,char *argv[])
{
printf("%d",argc);//argc 默认值为1
for(i=1;i<argc;i++)//这个循环不会执行因为argc就是1
for(i=0;i<argc;i++)//这个循环可以执行
 char *states[] = {
            "California", "Oregon",
            NULL, "Texas",NULL
      };
 
     argv[0]=states[0];//argv给states赋值
return 0;
}
